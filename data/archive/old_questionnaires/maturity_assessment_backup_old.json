{
  "questionnaire_id": "maturity_assessment",
  "name": "SE Maturity Assessment",
  "description": "Assess organizational SE maturity using Bretz model",
  "phase": "phase1_prepare_training",
  "estimated_time_minutes": 15,
  "questions": [
    {
      "id": "mat_001",
      "text": "How would you describe your organization's SE process documentation?",
      "type": "single_choice",
      "required": true,
      "weight": 0.15,
      "options": [
        {"value": "none", "label": "No documented SE processes", "score": 0},
        {"value": "basic", "label": "Basic documentation exists", "score": 1},
        {"value": "structured", "label": "Structured processes documented", "score": 2},
        {"value": "standardized", "label": "Standardized and widely used", "score": 3},
        {"value": "optimized", "label": "Continuously optimized processes", "score": 4}
      ]
    },
    {
      "id": "mat_002",
      "text": "To what extent are SE roles and responsibilities clearly defined?",
      "type": "single_choice",
      "required": true,
      "weight": 0.12,
      "options": [
        {"value": "undefined", "label": "Roles are not defined", "score": 0},
        {"value": "informal", "label": "Informally defined roles", "score": 1},
        {"value": "basic", "label": "Basic role definitions exist", "score": 2},
        {"value": "clear", "label": "Clear roles and responsibilities", "score": 3},
        {"value": "mature", "label": "Mature role structure with career paths", "score": 4}
      ]
    },
    {
      "id": "mat_003",
      "text": "How consistently are SE methods applied across projects?",
      "type": "single_choice",
      "required": true,
      "weight": 0.18,
      "options": [
        {"value": "never", "label": "SE methods not applied", "score": 0},
        {"value": "rarely", "label": "Occasionally on select projects", "score": 1},
        {"value": "sometimes", "label": "Applied on major projects", "score": 2},
        {"value": "usually", "label": "Applied consistently across most projects", "score": 3},
        {"value": "always", "label": "Universally applied and measured", "score": 4}
      ]
    }
  ],
  "scoring": {
    "method": "weighted_average",
    "scale": [0, 4],
    "maturity_levels": {
      "initial": [0, 0.8],
      "developing": [0.8, 1.6],
      "defined": [1.6, 2.4],
      "managed": [2.4, 3.2],
      "optimized": [3.2, 4.0]
    }
  }
}
