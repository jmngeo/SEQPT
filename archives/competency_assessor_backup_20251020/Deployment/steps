install git
sudo apt update
sudo apt install git -y

generate ssh key 
ssh-keygen -t rsa -b 4096 -C "derik.roby@outlook.com"


copy ssh key and add it in github
cat ~/.ssh/id_rsa.pub

check connectivity to git
 ssh -T git@github.com

clone the repo
git clone git@github.com:derik-roby/sesurveyapp.git

 docker compose build --no-cache
